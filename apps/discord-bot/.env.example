# Discord Bot Tokens
# 各キャラクター用のBotトークンを設定
USAKO_BOT_TOKEN=your_usako_bot_token_here
NEKOKO_BOT_TOKEN=your_nekoko_bot_token_here
KEROKO_BOT_TOKEN=your_keroko_bot_token_here

# Discord Settings
GUILD_ID=your_guild_id_here
CHANNEL_ID=your_channel_id_here  # テキストチャンネルID（旧設定、現在は未使用）

# Bot Settings
AUTO_CONVERSATION_START_HOUR=10
AUTO_CONVERSATION_END_HOUR=18
MESSAGE_INTERVAL_MIN=30
MESSAGE_INTERVAL_MAX=120
# REPORT_THRESHOLD=20  # 時間ベースのスケジュール実行に移行したため不要

# Ollama Settings
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen3:14b
OLLAMA_TEMPERATURE=0.8
OLLAMA_TOP_P=0.9
OLLAMA_REPEAT_PENALTY=1.1
OLLAMA_MAX_TOKENS=-1
# Character Settings
KEROKO_PERSONALITY=A

# Firebase Settings
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=
FIREBASE_MEASUREMENT_ID=

# === Voice/TTS Settings ===
# TTS APIのURL（Ollamaのエンドポイント）
TTS_API_URL=http://localhost:11434

# TTS機能を有効化（true/false）
TTS_ENABLED=false

# 音声チャンネルID（Discordから取得）※テキストメッセージもこのチャンネルに送信されます
VOICE_CHANNEL_ID=

# 音声チャンネル配信を有効化（true/false）
VOICE_CHANNEL_ENABLED=false
